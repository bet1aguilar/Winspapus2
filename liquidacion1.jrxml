	<queryString>
		<![CDATA[SELECT
     mpres.`nombre` AS mpres_nombre,
     mpres.`ubicac` AS mpres_ubicac,
     mprops.`logo` AS mprops_logo,
     mconts.`nombre` AS mconts_nombre,
     mconts.`encabe` AS mconts_encabe,
     mpres.`nrocon` AS mpres_nrocon,
     mconts.`cedrep` AS mconts_cedrep,
     mconts.`repleg` AS mconts_repleg,
     mconts.`logo` AS mconts_logo,
     mpres.`porimp` AS mpres_porimp,
     mvalus.`desde` AS mvalus_desde,
     mvalus.`hasta` AS mvalus_hasta
FROM
     `mprops` mprops INNER JOIN `mpres` mpres ON mprops.`id` = mpres.`codpro`
     INNER JOIN `mconts` mconts ON mpres.`codcon` = mconts.`id`
     INNER JOIN `mvalus` mvalus ON mpres.`id` = mvalus.`mpre_id`
WHERE
     mpres.`id` = $P{mpres}
 AND mvalus.`id` = $P{mvalu}
 AND mvalus.`mpre_id` = $P{mpres}]]>
	</queryString>
